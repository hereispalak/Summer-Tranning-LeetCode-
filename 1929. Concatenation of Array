class Solution {
public int[] getConcatenation(int[] nums) {
       int size = nums.length;
        int a[] = new int[size+size];
        
        	for(int i=0; i<size; i++)
            {
       	a[i]=nums[i];
            }
        for(int i=size, j=0; i<size+size; i++, j++)
            {
       	a[i]=nums[j];
            }
        return a;
    }
};
